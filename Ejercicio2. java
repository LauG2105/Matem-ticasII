mport java.util.*;

public class Main {
    public static void main(String[] args) {
      
      int promedio=0, num1=40, num2=50,num3=60, num4=24, num5=66, num6=12, num7=98;
      promedio = (num1 + num2 + num3 + num4 + num5 + num6 + num7) / 7;
      
      
      System.out.println("El promedio de los números es  " + promedio);
      
      if (num1 >=50) {
        System.out.println("El número esta por encima del promedio  " + num1);
      }else{
        System.out.println("EL número esta por debajo del promedio  " + num1);
      }
      if (num2 >=50) {
        System.out.println("El número esta por encima del promedio  " + num2);
      }else{
        System.out.println("EL número esta por debajo del promedio  " + num2);
      }
      if (num3 >=50) {
        System.out.println("El número esta por encima del promedio  " + num3);
      }else{
        System.out.println("EL número esta por debajo del promedio  " + num3);
      }
      if (num4 >=50) {
        System.out.println("El número esta por encima del promedio  " + num4);
      }else{
        System.out.println("EL número esta por debajo del promedio  " + num4);
      }
      if (num5 >=50) {
        System.out.println("El número esta por encima del promedio  " + num5);
      }else{
        System.out.println("EL número esta por debajo del promedio  " + num5);
      }
      if (num6 >=50) {
        System.out.println("El número esta por encima del promedio  " + num6);
      }else{
        System.out.println("EL número esta por debajo del promedio  " + num6);
      }
      if (num7 >=50) {
        System.out.println("El número esta por encima del promedio  " + num7);
      }else{
        System.out.println("EL número esta por debajo del promedio  " + num7);
      }
      
  }
}
